<!DOCTYPE html>

<html>
<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="author" content="{{meta.studioName}}">
  <meta name="generator" content="mPress">
  <meta property="og:site_name" content="{{meta.projectName}} by {{meta.studioName}}">
  <meta name="twitter:site" content="{{meta.website}}">
  <meta name="twitter:creator" content="{{meta.studioName}}">

  <meta property="og:title" content="{{meta.projectName}} by {{meta.studioName}}">
  <meta name="twitter:title" content="{{meta.projectName}} by {{meta.studioName}}">
  <meta property="og:url" content="{{meta.url}}">
  <meta name="description" content="{{meta.description}}">
  <meta property="og:description" content="{{meta.description}}">
  <meta name="twitter:description" content="{{meta.description}}">
  <meta property="og:image" content="{{meta.shareImage}}">
  <meta name="twitter:image" content="{{meta.shareImage}}">
  <meta property="og:type" content="article">
  <title>{{meta.studioName}}</title>
  <meta description="{{meta.description}}" />

  <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,700,800,900&display=swap" rel="stylesheet">

  <style>
  body {
    padding: 15px 0;

    font-size: 0.9em;
    line-height:1.6em;
    font-family: 'Roboto Slab', serif !important;
  }

  h1, h2, h3, h4{
    font-weight: 800 !important;
  }

  img{
    max-width: 100%;
  }



  .image-square img {
    max-height: 150px;
  }

  h2 {
      margin-top: 35px !important;
  }

  .laurel {
    max-width: 340px;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    flex-direction: row;
    flex-flow: row no-wrap;
    position: relative;
    color: #000000;
    text-align: center;
    padding: 7px;
    margin: 15px auto 0 auto;

    font-weight: 700;
    text-transform: uppercase;
    max-height: 300px;
    flex-wrap: nowrap;
    justify-content: space-around;
    align-items: center;
    vertical-align: middle;
  }
  .laurel .textwidget {
    padding: 0 5px;
    flex-grow: 5;
    width: 80%;
    font-size: 18px;
  }
  .laurel .laurel-left, .laurel .laurel-right {
    width: 10%;
    flex-grow: 3;
    flex: 1 auto;
    vertical-align: middle;
  }
  .laurel .laurel-left svg, .laurel .laurel-right svg {
    position: relative;
    max-height: 120px;
  }
  .laurel .laurel-left svg path, .laurel .laurel-right svg path {
    fill: black;
  }
  .laurel .laurel-left {
    justify-content: right;
  }
  .laurel .laurel-left svg {
    float: right;
  }
  .laurel .laurel-right {
    -moz-transform: scaleX(-1);
    -o-transform: scaleX(-1);
    -webkit-transform: scaleX(-1);
    transform: scaleX(-1);
    filter: FlipH;
    -ms-filter: "FlipH";
  }

  .image-container{
    margin-top: 15px;

  }

  .image-container .caption{
    text-align: center;
    font-size: 0.8rem;
  }

  .main{
    padding-left: 15px;
  }


  </style>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
</head>

<body>
  <div class="container">

  <div class="row">
    <div class="col-sm-4 sidebar">
      <h1>{{meta.projectName}}</h1>
      <a href="{{meta.websiteURL}}" target="_blank">{{meta.websiteName}}</a>

      <p><strong>Developer:</strong><br />
        <a href="{{meta.websiteURL}}" target="_blank">{{meta.studioName}}</a><br />
        {{#if meta.address}}
        Based in {{meta.address.city}}, {{meta.address.state}},
        {{meta.address.country}}
        {{/if}}
      </p>


      <!-- GENRE -->
      {{#if meta.genre}}
      <p><strong>Genre:</strong><br />
        {{meta.genre}}</p>
        {{/if}}

        <!-- IN DEVELOPMENT SINCE -->
        {{#if meta.dateStarted}}
        <p><strong>In Development since:</strong><br />
          {{meta.dateStarted}}</p>
          {{/if}}
          <!-- PRESS EMBARGO DATE -->
          {{#if meta.dateStarted}}
          <p><strong>Press Embargo Date:</strong><br />
            {{meta.pressEmbargoDate}}</p>
            {{/if}}


          <!-- RELEASE DATES -->

          {{#if meta.releaseDate}}
          <strong>Release Date:</strong><br />
          <ul>
            {{#each meta.releaseDate}}
            {{#if show}}
            <li>{{platform}} - {{date}}</li>
            {{/if}}
            {{/each}}
          </ul>
          {{/if}}

          <!-- LANGUAGES -->

          {{#if meta.languages}}
          <p>
            <strong>languages:</strong>
            <ul>
            {{#each meta.languages}}
            <li>{{this}}</li>
            {{/each}}
            </ul>
          </p>
          {{/if}}

          <!-- QUICK FACTS -->
          {{#if meta.quickFacts}}
          <strong id="quickFacts">Quick Facts:</strong>
          <ul>{{#each meta.quickFacts}}
            <li>{{this}}</li>
            {{/each}}
          </ul>
          {{/if}}

          <!-- Website -->
          {{#if meta.websiteURL}}
          <p><strong>Website:</strong><br />
            <a href="{{meta.websiteURL}}" target="_blank">{{meta.websiteURL}}</a></p>
            {{/if}}

            <!-- SOCIAL -->

            {{#if social}}
            <p>
              <strong>Social:</strong>
              <br />
              {{#each social}}
              <a href="{{url}}" target="_blank">{{name}}</a>
              <br />
              {{/each}}
            </p>
            {{/if}}


            <!-- PRESS CONTACT -->
            {{#if meta.pressContact}}
            <p><strong>Press / Business contact:</strong><br />
              {{meta.pressContact.name}} - {{meta.pressContact.email}}</p>
              {{/if}}


            </div> <!-- end sidebar -->




            <div class="col-sm-8 Main">
              <img class="keyart" src="{{meta.keyArt}}">
              <h2 id="details">{{meta.projectType}} Details</h2>
              <!-- ONE LINER -->
              {{#if meta.oneLiner}}
              <strong id="oneLiner">One Liner:</strong>
              <p>{{{meta.oneLiner}}}</p>
              {{/if}}
              <!-- PITCH -->
              {{#if meta.pitch}}
              <strong id="pitch">Pitch:</strong>
              <p>{{{meta.pitch}}}</p>
              {{/if}}

              <!-- FEATURES -->
              {{#if meta.features}}
              <strong id="features">Features:</strong>
              <ul>{{#each meta.features}}
                <li>{{this}}</li>
                {{/each}}
              </ul>
              {{/if}}

              <!-- TEAM -->
              {{#if team}}
              <h3 id="team">Team</h3>
              {{#each team}}
              <strong>{{name}}</strong>{{#if company}} - <strong>{{company}}</strong> {{/if}}
              <p>{{role}}</p>
              {{/each}}
              {{/if}}

            </div> <!-- end col-sm-8 -->
          </div> <!-- end row -->


          <hr />

          {{#if videos}}
          <h2 id="videos">Videos</h2>
          {{#each videos}}
          <div class="col-sm-6">

            <iframe width="560" height="315" src="http://www.youtube.com/embed/{{youtubeID}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            <p><strong>{{name}}</strong>&nbsp;<a href=
              "http://www.youtube.com/watch?v={{youtubeID}}" target="_blank">View on YouTube</a></p>
            </div>
            {{/each}}
            <hr />
            {{/if}}

            {{#if gifs}}
            <h2 id="gifs">Gifs</h2>
            <div class="gifs row">
              {{#each gifs}}
              <div class="col-sm-12 col-md-6 col-lg-4 image-container">
                <img src="{{gif}}" alt="{{caption}}" />
                {{#if caption}}<p class="caption">{{caption}}</p>{{/if}}
              </div>
              {{/each}}
          </div>
          <hr />
          {{/if}}

            {{#if images}}
            <h2 id="images">Images</h2>
            <div class="images row">
              {{#each images}}
              <div class="col-sm-12 col-md-6 col-lg-4 image-container">
                <a href="{{url}}" target="_blank"><img src="{{image}}" alt="{{caption}}" /></a>
                {{#if caption}}<p class="caption">{{caption}}</p>{{/if}}
              </div>
              {{/each}}
          </div>
          <hr />
          {{/if}}


          {{#if logos}}
          <div class=" images">
            {{#each logos}}
            <div class="">
              <a href="{{url}}" target="_blank"><img src="{{image}}" alt="{{alt}}" /></a>
            </div>
            {{/each}}
          </div>
          <hr />
          {{/if}}


          {{#if awardsAndRecognition}}
          <h2 id="awards">Awards &amp; Recognition</h2>

          <div class="row">
            {{#each awardsAndRecognition}}

            <div class="col-sm-12 col-md-6 col-lg-4">
              <div class="laurel widget widget_text">
